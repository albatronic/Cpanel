{# _global/menuIconosIzquierda.html.twig #}
<div id="primary_nav" class="g_blue">
    <ul>

        {# SI ES SUPER MUESTRO VARIABLES DE ENTORNO #}
        {% if user.idPerfil == 1 %}
            {% if not values.enCurso.app %}
                {# VARIABLES DE ENTORNO DEL PROYECTO#}
                <li><a href="{{app.path}}/CoreVariables/Env/Pro/{{values.project.name}}" title="Variables de Entorno Proyecto"><span class="icon_block m_dashboard">Variables de Entorno del Proyecto</span></a></li>
            {% else %}
                {# VARIABLES DE ENTORNO DE LA APP #}
                <li><a href="{{app.path}}/CoreVariables/Env/App/{{values.enCurso.app}}" title="Variables de Entorno App"><span class="icon_block m_dashboard">Variables de Entorno de la App</span></a></li>
                {% if values.enCurso.modulo %}
                {# VARIABLES DE ENTORNO DEL MODULO #}
                <li><a href="{{app.path}}/CoreVariables/Env/Mod/{{values.enCurso.modulo}}" title="Variables de Entorno Modulo"><span class="icon_block m_events">Variables de Entorno del Módulo</span></a></li>
                {% endif %}
            {% endif %}
        {% endif %}

        {# SI TIENE PERMISO 'VW' MUESTRO VARIABLES WEB #}
        {% if values.permisos['VW'] %}
            {% if not values.enCurso.app %}
                {# VARIABLES WEB DEL PROYECTO#}
                <li><a href="{{app.path}}/CoreVariables/Web/Pro/{{values.project.name}}" title="Variables Web Proyecto"><span class="icon_block m_events">Variables Web del Proyecto</span></a></li>
            {% else %}
                {# VARIABLES WEB DE LA APP #}
                <li><a href="{{app.path}}/CoreVariables/Web/App/{{values.enCurso.app}}" title="Variables Web App"><span class="icon_block m_events">Variables Web de la App</span></a></li>
                {% if values.enCurso.modulo %}
                {# VARIABLES WEB DEL MODULO #}
                <li><a href="{{app.path}}/CoreVariables/Web/Mod/{{values.enCurso.modulo}}" title="Variables Web Modulo"><span class="icon_block m_events">Variables Web del Módulo</span></a></li>
                {% endif %}
            {% endif %}
        {% endif %}

        <li><a href="#" title="Events"><span class="icon_block m_events">Events</span></a></li>
        <li><a href="#" title="Address Book"><span class="icon_block p_book">Address Book</span></a></li>
        <li><a href="#" title="Media"><span class="icon_block m_media">Media</span></a></li>
        <li><a href="#" title="Settings"><span class="icon_block m_settings">Settings</span></a></li>
    </ul>
</div>